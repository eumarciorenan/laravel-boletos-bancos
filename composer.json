{
    "name": "marciorenan/laravel-boletos-bancos",
    "description": "Biblioteca com boletos para o laravel",
    "keywords": [
        "marciorenan",
        "laravel-boletos-bancos"
    ],
    "homepage": "https://github.com/marciorenan/laravel-boletos-bancos",
    "license": "MIT",
    "authors": [
        {
            "name": "MÃ¡rcio Renan Almeida",
            "email": "eumarciorenan@gmail.com"
        }
    ],
    "require": {
        "php": ">7.4.0",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf": "^1.8",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "marciorenan/laravel-mail-auto-embed": "dev-master",
        "chillerlan/php-qrcode": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "friendsofphp/php-cs-fixer": "*"
    },
    "autoload": {
        "psr-4": {
            "marciorenan\\LaravelBoletosBancos\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "marciorenan\\LaravelBoletosBancos\\Tests\\": "tests"
        }
    },
    "config": {
        "composer/installers": true,
        "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "scripts": {
        "php-cs-fixer-check": "vendor/bin/php-cs-fixer fix --diff --verbose --dry-run",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --verbose"
    }
}
